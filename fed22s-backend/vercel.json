{
  "version": 2,
  "builds": [
    { "src": "fed22s-backend/*.ts", "use": "@vercel/node" },
    { "src": "fed22s-backend/src/**/*.ts", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/fed22s-backend/src/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/fed22s-backend/src/$1"
    }
  ]
}
